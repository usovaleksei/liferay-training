<?xml version="1.0"?>

<model-hints>
	<model name="com.liferay.library.model.Book">
		<field name="uuid" type="String" />
		<field name="bookId" type="long" />
		<field name="authorId" type="long" />
		<field name="bookTitle" type="String" />
		<field name="createDate" type="Date" />
		<field name="genreId" type="long" />
		<field name="studentId" type="long" />
	</model>
	<model name="com.liferay.library.model.Genre">
		<field name="uuid" type="String" />
		<field name="genreId" type="long" />
		<field name="title" type="String" />
	</model>
	<model name="com.liferay.library.model.Student">
		<field name="uuid" type="String" />
		<field name="studentId" type="long" />
		<field name="name" type="String" />
